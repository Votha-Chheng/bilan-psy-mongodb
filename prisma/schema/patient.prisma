enum Sexe {
  m
  f
}

model Patient {
  id                  String              @id @default(cuid()) @map("_id")
  user                User                @relation(fields: [userId], references: [id])
  userId              String              
  nom                 String
  prenom              String
  dateNaissance       String
  sexe                Sexe    
  adulte              Boolean
  medecin             String?  
  motif               String?
  ecole               String?
  createdAt           DateTime            @default(now())
  updated             DateTime            @updatedAt()
  dateBilan           String?
  bilan               Bilan?
  // conclusion          Conclusion?
  anamnese            Anamnese?   
  // amenagements        AmenagementsPlan[]
}
